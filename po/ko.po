msgstr "\"%s\"은(는) 올바른 프로그램 ID가 아닙니다"

#: ../src/gterminal.vala:102
msgid "Server application ID"
msgstr "서버 프로그램 ID"

#: ../src/gterminal.vala:102
msgid "ID"
msgstr "<ID>"

