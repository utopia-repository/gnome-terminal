msgstr "Gặp lỗi khi xử lý đối số: %s\n"

#: ../src/migration.c:403
msgid "Default"
msgstr "Mặc định"

#: ../src/migration.c:403 ../src/terminal-prefs.c:99
msgid "Unnamed"
msgstr "Không tên"

